{
  "name": "gistbook",
  "version": "0.2.1",
  "description": "A place to share code snippets on the web.",
  "main": "index.js",
  "scripts": {
    "postinstall": "bower install",
    "test": "grunt test",
    "get-secrets": "./.bin/get-secrets",
    "configure-hosts-local": "hostile set 127.0.0.1 gistbook.loc",
    "provision": "grunt build:prod && ansible-playbook -i deploy/ansible/inventory/production deploy/ansible/provision.yml",
    "provision-staging": "grunt build:prod && ansible-playbook -i deploy/ansible/inventory/staging deploy/ansible/provision.yml",
    "deploy": "grunt build:prod && ansible-playbook -i deploy/ansible/inventory/production deploy/ansible/deploy.yml",
    "deploy-staging": "grunt build:prod && ansible-playbook -i deploy/ansible/inventory/staging deploy/ansible/deploy.yml"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Gistbook/gistbook.git"
  },
  "keywords": [
    "gist",
    "gistbook",
    "gists",
    "github",
    "javascript",
    "html",
    "css",
    "interactive",
    "snippets",
    "code",
    "social",
    "sharing"
  ],
  "author": "Jmeas",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Gistbook/gistbook/issues"
  },
  "homepage": "https://github.com/Gistbook/gistbook",
  "devDependencies": {
    "6to5": "^1.10.10",
    "6to5-loader": "^0.2.3",
    "bach": "^0.4.0",
    "bower": "^1.3.12",
    "chalk": "^0.5.1",
    "exports-loader": "^0.6.2",
    "grunt": "~0.4.2",
    "grunt-contrib-clean": "~0.5.0",
    "grunt-contrib-copy": "~0.5.0",
    "grunt-contrib-cssmin": "^0.10.0",
    "grunt-contrib-imagemin": "~0.5.0",
    "grunt-contrib-jshint": "git://github.com/jmeas/grunt-contrib-jshint.git#hi-es6-modules",
    "grunt-contrib-stylus": "^0.15.1",
    "grunt-contrib-uglify": "~0.3.2",
    "grunt-contrib-watch": "~0.6.0",
    "grunt-express-server": "~0.4.13",
    "grunt-webpack": "^1.0.5",
    "hostile": "^1.0.0",
    "imports-loader": "^0.6.2",
    "load-grunt-tasks": "~0.4.0",
    "mkdirp": "^0.5.0",
    "ssh2": "^0.3.6",
    "userhome": "^1.0.0"
  },
  "dependencies": {
    "backbone": "^1.1.2",
    "backbone.babysitter": "^0.1.1",
    "backbone.base-router": "^0.4.1",
    "backbone.intercept": "^0.3.0",
    "backbone.marionette": "^2.3.0-pre",
    "backbone.radio": "^0.6.0",
    "body-parser": "^1.7.0",
    "browser-module-cache": "^0.1.3",
    "cookies": "^0.4.1",
    "cookies-js": "^0.4.0",
    "detective": "^4.0.0",
    "emojify.js": "^0.9.5",
    "express": "^4.0.0",
    "express-handlebars": "^1.1.0",
    "express-routebuilder": "^2.0.0",
    "grunt-contrib-handlebars": "^0.9.1",
    "handlebars": "^2.0.0",
    "jquery": "^2.1.0",
    "json-loader": "^0.5.1",
    "marked": "^0.3.2",
    "octonode": "^0.6.2",
    "semantic-ui": "^1.1.2",
    "sortable": "git://github.com/jmeas/Sortable.git#optional-set-text",
    "spin.js": "^2.0.1",
    "underscore": "1.7.0"
  }
}
